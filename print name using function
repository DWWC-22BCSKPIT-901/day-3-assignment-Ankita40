#include <iostream>
#include <string>
using namespace std;

// Function to print the full name
void print_full_name(string first, string last) {
    cout << "Hello " << first << " " << last << "! You just delved into function" << endl;
}

int main() {
    string first, last;
    cin >> first; // Input first name
    cin >> last;  // Input last name
    print_full_name(first, last); // Call the function to print the full name
    return 0;
}
